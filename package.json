{
    "name": "template-express-ts",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --watch \"src/**/*.ts\" --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings -r tsconfig-paths/register\" src/server.ts"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts, json",
        "exec": "ts-node -r tsconfig-paths/register src/server.ts"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-jwt": "^8.4.1",
        "jsonwebtoken": "^9.0.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui": "^5.18.2",
        "tsconfig-paths": "^4.2.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^22.5.4",
        "nodemon": "^3.1.4",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^5.6.3"
    }
}